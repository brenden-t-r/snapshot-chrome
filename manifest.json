{
  "name": "Basic Chrome extension",
  "description": "Print inside console when clicked",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Print inside console when clicked"
  },
  "manifest_version": 2,
   "commands": {
        "testShortkut": {
            "suggested_key": {
              "default": "Ctrl+Shift+3"
            }, "description": "A test shortcut"
          }
   }
}
